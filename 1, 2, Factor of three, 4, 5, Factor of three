import java.util.Scanner;

public class AlterFactor_of_three {
	public static void main(String arg[])
	{
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		int c=0;
		for(int i=1;i<=a;i++)
		{
			if(i%3==0)
			{
				System.out.print(", Factor of three");
			}
			else
			{
				c++;
				if(c!=1)
				{
					System.out.print(", ");
				}
				System.out.print(i);
			}
		}
	}
}
