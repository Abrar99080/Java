public class FreqDist {
    public static void main(String[] args) {
        int a[] = {1,2,3,4,2,1,6,8};
        for(int i=0;i<a.length;i++)
        {
            int f=0;
            for(int j=i;j>=0;j--)
            {
                if(a[i]==a[j])
                    f++;
            }
            if(f==1)
            {
                System.out.print(a[i]+" ");
            }
        }
    }
}
