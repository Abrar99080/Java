package innstack;

import java.util.Scanner;

public class evenOdd {
	public static void main(String arg[])
	{
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		for(int i=1;i<=a;i++)
		{
			if(i%2==0)
			{
				System.out.print("even");
				if(i<a)
				{
					System.out.print(", ");
				}
			}
			else
			{
				System.out.print(i);
				if(i<a)
				{
					System.out.print(", ");
				}
			}
		}
	}
}
