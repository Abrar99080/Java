public class pairs {
    public static void main(String[] args) {
        int a[] = {2,2,2,4,3,4,5,5,6,7,4,5,2,2};
        for(int i=0;i<a.length;i++)
        {
            int f=0;
            for(int j=i;j>=0;j--)
            {
                if(a[i]==a[j])
                {
                    f++;
                }
            }
            if(f%2==0)
            {
                System.out.println(a[i]+" "+a[i]);
            }
        }
    }
}
